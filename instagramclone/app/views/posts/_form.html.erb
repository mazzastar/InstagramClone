<%= form_for @post, html: {multipart: true} do |f| %>
	
	<%= f.label :title %>
	<%= f.text_area :title %>

	<%= f.label :caption %>
	<%= f.text_area :caption %>

	<%= f.label :picture %>
	<%= f.file_field :picture %>

	<%= f.label :tag_names, 'hashtags' %>
	<%= f.text_area :tag_names %>

 
	<%= f.submit 'Leave Review' %>



<% end %>